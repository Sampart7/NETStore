@page "{reference}"
@model RecordStore.UI.Pages.OrderModel
@{
}

<div>
    <h1>@Model.Order.OrderRef</h1>

    @foreach (var product in Model.Order.Products)
    {
        <div>Name - @product.Name</div>
        <div>Description - @product.Description</div>
        <div>Qty - @product.Qty</div>
        <div>Value - @product.Value</div>
        <div>StockDescription - @product.StockDescription</div>
    }
</div>